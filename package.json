{
  "name": "Server",
  "version": "3.0.0",
  "author": "AE-Team, JET Team, Paulov",
  "license": "MIT",
  "scripts": {
    "start": "node bin/www.js",
    "generate-docs": "jsdoc -c jsdoc.json -r .",
    "package": "pkg .",
    "debug": "node bin/www.js --trace-warnings"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "decimal-adjust": "^0.0.0",
    "docdash": "^1.2.0",
    "ejs": "^3.1.7",
    "express": "^4.18.1",
    "https": "^1.0.0",
    "mathjs": "^10.6.1",
    "selfsigned": "^2.0.1",
    "uuid": "^8.3.2",
    "write-json-file": "^4.3.0",
    "ws": "^8.8.1"
  },
  "bin": "bin/www.js",
  "pkg": {
    "scripts": [
      "core/**/*.js",
      "src/**/*.js"
    ],
    "assets": [
      "res/**/*",
      "db/**/*",
      "public/**/*",
      "express/views/**/*"
    ],
    "targets": [
      "node16-win"
    ]
  }
}
